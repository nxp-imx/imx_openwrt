+
+	pinctrl_lpi2c8: lpi2c8grp {
+		fsl,pins = <
+			MX91_PAD_GPIO_IO12__LPI2C8_SDA		0x40000b9e
+			MX91_PAD_GPIO_IO13__LPI2C8_SCL		0x40000b9e
+		>;
