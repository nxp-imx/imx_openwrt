+
+&cameradev {
+	parallel_csi;
+	status = "okay";
+};
+
+&isi_0 {
+	interface = <6 0 2>;
+	status = "okay";
+
+	cap_device {
+		status = "okay";
+	};
+};
+
+&parallel_csi {
+	status = "okay";
+
+	port {
+		parallel_csi_ep: endpoint {
+			remote-endpoint = <&mt9m114_ep>;
