+
+&flexspi0 {
+	pinctrl-names = "default";
+	pinctrl-0 = <&pinctrl_flexspi0>;
+	status = "okay";
+	nxp,fspi-individual-mode;
+
+	mt25qu512abb0: flash@0 {
+		reg = <0>;
